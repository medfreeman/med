    "remark-slug": "^4.2.2",
    "remark-toc": "^4.0.0",
    "webpack": "^2.6.1"
  },
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore --fix .",
    "lint": "npm-run-all --parallel lint:*",
    "test": "yarn run lint",
    "start": "cross-env NODE_ENV=development phenomic start",
    "build": "phenomic build"
  },
  "babel": {
    "presets": [
      "@phenomic/babel-preset"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": "eslint-config-i-am-meticulous/react",
    "rules": {
      "indent": [
        "error",
        2
      ],
      "quotes": [
        "error",
        "double",
        {
          "allowTemplateLiterals": true
        }
      ],
      "semi": [
        "error",
        "never"
      ],
      "react/jsx-curly-spacing": [
        "error",
        "always"
      ],
      "react/jsx-equals-spacing": [
        "error",
        "never"
      ]
    }
  }
}
