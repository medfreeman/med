  "scripts": {
    "install": "napa Templarian/MaterialDesign:mdi",
    "lint:js": "eslint --ignore-path .gitignore --fix .",
    "lint:css": "stylelint \"src/**/*.css\" --fix",
    "lint": "npm-run-all --parallel lint:*",
    "prettier": "prettier --write --parser=flow \"src/**/*.@(js|jsx)\"",
    "test": "yarn run lint",
    "start": "phenomic start",
    "build": "phenomic build",
    "build-for-deploy": "cross-env URL=https://medfreeman.github.io phenomic build"
  },
  "babel": {
    "presets": [
      "@phenomic/babel-preset"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": "eslint-config-i-am-meticulous/react-flow",
    "globals": {
      "PHENOMIC_URL": true,
      "RECAPTCHA_SITE_KEY": true
    },
    "rules": {
      "import/no-extraneous-dependencies": [
        "off"
      ],
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "(Elements|Layout|Meta|Templates|icons|utils)/.*$",
            "package.json"
          ]
        }
      ],
      "import/order": [
        "off"
      ],
      "import/max-dependencies": [
        2,
        {
          "max": 20
        }
      ]
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-css-modules"
    ],
    "rules": {
      "declaration-empty-line-before": null,
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "context",
            "import-normalize"
          ]
        }
      ]
    }
  }
